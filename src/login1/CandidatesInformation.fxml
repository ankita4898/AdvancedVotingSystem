<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" fx:id="candidateinformationPane" prefHeight="700.0" prefWidth="1000.0" 
            styleClass="root" stylesheets="@../CSS/CandidateInformation.css" 
            xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" 
            fx:controller="login1.CandidatesInformationController">
     <children>
      <Label alignment="CENTER" layoutX="110.0" prefHeight="59.0" prefWidth="874.0" text="CANDIDATE INOFORMATION" AnchorPane.leftAnchor="110.0" AnchorPane.topAnchor="10.0">
