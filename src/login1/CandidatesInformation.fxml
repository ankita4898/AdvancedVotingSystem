            <Font name="Arial Black" size="40.0" />
         </font>
      </Label>
      <TableView fx:id="candidate_table" layoutX="40.0" layoutY="91.0" prefHeight="400.0" prefWidth="900.0" stylesheets="@../CSS/CandidateInformation.css" AnchorPane.leftAnchor="40.0" AnchorPane.rightAnchor="40.0" AnchorPane.topAnchor="91.0">
        <columns>
